<body id="<%- data.header.index.slug %>" class="js-anime">
    <div class="js-media-query u-media-query"></div>
    <div class="js-nav-overlay nav-overlay"></div>
    <div class="l-wrapper">
        <header class="js-header l-header" itemscope itemtype="http://schema.org/WPHeader">
            <div class="hd-info">
                <div class="hd-info-inner">
                    <h1 class="hd-info__logo js-logo">
                        <a href="/" class="hd-info_logo-link u-alpha"><%- data.header.index.title %></a>
                    </h1>
                    <ul class="js-nav-btn nav-btn">
                        <li class="nav-btn__line"></li>
                        <li class="nav-btn__line"></li>
                        <li class="nav-btn__line"></li>
                    </ul>
                    <nav class="js-nav-content l-nav animated-nav" itemscope itemtype="http://www.schema.org/SiteNavigationElement">
                        <ul class="nav-list">
                            <li class="nav-list__item" itemprop="name">
                                <a class="nav-list__link" itemprop="URL" href="#about">ABOUT</a>
                            </li>
                            <li class="nav-list__item" itemprop="name">
                                <a class="nav-list__link" itemprop="URL" href="#profile">PROFILE</a>
                            </li>
                            <li class="nav-list__item" itemprop="name">
                                <a class="nav-list__link" itemprop="URL" href="#works">WORKS</a>
                            </li>
                            <li class="nav-list__item" itemprop="name">
                                <a class="nav-list__link" itemprop="URL" href="#skill">SKILL</a>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
        </header>
        <div class="l-main-img">
            <picture>
                <source srcset="<%- data.header.index.imgSrc %>/<%- data.header.index.slug %>/img_main01.webp" type="image/webp">
                <img src="<%- data.header.index.imgSrc %>/<%- data.header.index.slug %>/img_main01.png" alt="<%- data.header.index.title %>">
            </picture>
			<div class="l-main-img-inner">
				<p class="main-txt"><%- data.header.index.title %></p>
			</div>
		</div>